;\r\n\r\nexport default {\r\n  data: () => ({\r\n    categories: [],\r\n    errorMessages: '',\r\n    post: null,\r\n    category: null,\r\n    formHasErrors: false,\r\n  }),\r\n  created() {\r\n    this.fetchCategories();\r\n  },\r\n  computed: {\r\n    form () {\r\n      return {\r\n        post: this.post,\r\n        category: this.category,\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    async fetchCategories() {\r\n      try {\r\n        const response = await axios.get('http://127.0.0.1:8080/categories');\r\n        this.categories = response.data;\r\n      } catch (error) {\r\n        console.error('Categories fetching error.', error);\r\n      }\r\n    },\r\n    postCheck () {\r\n      this.errorMessages = this.post\r\n          ? `Hey! I'm required...`\r\n          : ''\r\n\r\n      return true\r\n    },\r\n    resetForm () {\r\n      this.formHasErrors = false\r\n\r\n      Object.keys(this.form).forEach(f => {\r\n        this.$refs[f].reset()\r\n      })\r\n    },\r\n    async share() {\r\n      const formData = {\r\n        PostBody: this.post,\r\n        CategoryName: this.category\r\n      };\r\n\r\n      try {\r\n        const response = await axios.post('http://127.0.0.1:8080/post', formData);\r\n        console.log('Data sent successfully.', response.data);\r\n\r\n        this.resetForm();\r\n      } catch (error) {\r\n        console.error('Data sending error.', error);\r\n        this.formHasErrors = true;\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AAsFA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAEA,CAAA,MAAO;IACXC,UAAU,EAAE,EAAE;IACdC,aAAa,EAAE,EAAE;IACjBC,IAAI,EAAE,IAAI;IACVC,QAAQ,EAAE,IAAI;IACdC,aAAa,EAAE;EACjB,CAAC,CAAC;EACFC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,QAAQ,EAAE;IACRC,IAAGA,CAAA,EAAK;MACN,OAAO;QACLN,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,QAAQ,EAAE,IAAI,CAACA;MACjB;IACF;EACF,CAAC;EACDM,OAAO,EAAE;IACP,MAAMH,eAAeA,CAAA,EAAG;MACtB,IAAI;QACF,MAAMI,QAAO,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAAC,kCAAkC,CAAC;Q